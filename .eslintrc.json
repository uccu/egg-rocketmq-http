{
    "extends": "eslint-config-egg/typescript",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "globals": {
      "NormalException": "readonly"
    },
    "rules": {
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/ban-ts-comment": 0,
      "no-var": 0,
      "no-bitwise": 0,
      "no-constant-condition": 0
    }
  }